{
  "name": "angular-cli",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"npm run client\" \"npm run cms\"",
    "cms": "ng serve --project=cms --port 8080",
    "client": "ng serve --project=client --port 8081 ",
    "build": "concurrently \"npm run build:client\" \"npm run build:cms\"",
    "build:cms": "ng build cms -p",
    "build:client": "ng build client -p ",
    "build:core": "ng build client -p",
    "build:extern": "ng build extern -p",
    "test": "karma start ./karma.conf.js",
    "lint": "ng lint",
    "e2e": "protractor ./protractor.conf.js",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "6.0.4",
    "@angular/common": "6.0.4",
    "@angular/compiler": "6.0.4",
    "@angular/core": "6.0.4",
    "@angular/forms": "6.0.4",
    "@angular/http": "6.0.4",
    "@angular/platform-browser": "6.0.4",
    "@angular/platform-browser-dynamic": "6.0.4",
    "@angular/platform-server": "6.0.4",
    "@angular/router": "6.0.4",
    "@types/jquery": "^3.3.2",
    "bootstrap": "^4.1.1",
    "bootstrap-material-design": "^4.1.1",
    "ckeditor": "^4.6.2",
    "codemirror": "^5.28.0",
    "core-js": "^2.4.1",
    "dragula": "^3.7.2",
    "jquery": "^3.3.1",
    "jquery-ui-bundle": "^1.11.4",
    "ng-packagr": "^3.0.1",
    "ng2-codemirror": "^1.1.3",
    "ng2-dragula": "^1.3.0",
    "ngx-textarea-autosize": "^1.1.1",
    "rxjs": "^6.2.0",
    "rxjs-compat": "^6.2.0",
    "typescript": "2.7.2",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "concurrently": "^3.4.0",
    "@angular/compiler-cli": "^6.0.3",
    "@angular-devkit/build-ng-packagr": "~0.6.6",
    "@angular-devkit/build-angular": "~0.6.5",
    "tsickle": ">=0.25.5",
    "tslib": "^1.7.1",
    "typescript": "~2.7.2",
    "@angular/cli": "~6.0.5",
    "@angular/language-service": "^6.0.3",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1"
  }
